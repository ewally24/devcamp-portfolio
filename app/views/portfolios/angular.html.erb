<h1> Angular Portfolio Items </h1>

<% @angular_portfolio_items.each do |portfolio_item|%>
  <%= image_tag portfolio_item.main_image unless portfolio_item.main_image.nil?%>
  <h1><%= link_to portfolio_item.title, portfolio_show_path(portfolio_item.id) %></h1>
  <em><%= portfolio_item.subtitle %></em>
  <p><%= portfolio_item.body %></em>
   <%= link_to 'edit', edit_portfolio_path(portfolio_item.id) %>
<% end %>
